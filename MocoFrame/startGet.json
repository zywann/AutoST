[
  {
    "description":"这是无参的get请求",
    "request":{
      "uri":"/getDemo",
      "method":"get"
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"这是无参的get请求"
    }
  },
  {
    "description":"这是一个有参的get请求",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries":{
        "name":"zhangsan",
        "age":"20"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"我张三回来了！！"
    }
  }
]