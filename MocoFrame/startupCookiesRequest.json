[
  {
    "description":"带cookies的get请求",
    "request":{
      "uri":"/get/with/cookies",
      "method":"get",
      "cookies":{
        "cookies":"cookies123456"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "json":{
        "张三":"登录成功",
        "status":"1"
      }
    }
  },
  {
    "description":"这是带参数带cookies的post请求",
    "request":{
      "uri":"/get/with/post/cookies",
      "method":"post",
      "cookies":{
        "cookies":"cookies123456"
      },
      "json":{
        "name":"zhangsan",
        "age":"19"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "json":{
        "zhangsan":"登录成功",
        "status":"1"
      }
    }
  },
  {
    "description":"这是带headers的post请求",
    "request":{
      "uri":"/post/with/headers",
      "method":"post",
      "headers":{
        "contend-type":"application/json"
      },
      "cookies":{
        "cookies":"cookies123456"
      },
      "json":{
        "name":"zhangsan",
        "age":"20"
      }
    },
    "response":{
      "headers":{
        "Contend-Type":"text/htmo;charset=gbk"
      },
      "json":{
        "status":"1",
        "zhangsan":"登录成功！"
      }
    }
  }

]