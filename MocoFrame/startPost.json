[
  {
    "description":"这是第一个post请求demo",
    "request":{
      "uri":"/postDemo",
      "method":"post"
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"这是一个无参的post请求"
    }
  },
  {
    "description":"这是一个有参post请求",
    "request":{
      "uri":"/postwithparam",
      "method":"post",
      "forms":{
        "name":"zhangsan",
        "age":"18"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"我张三又回来了！"
    }
  }
]